{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/.expo/types/**/*.ts",
    "src/expo-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
